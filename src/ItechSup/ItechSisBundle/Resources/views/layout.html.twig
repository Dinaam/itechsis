{% extends '::base.html.twig' %}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Administration - GSI Formation{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
        <div id="layout">
            <div class="menu">
                {% block menu %}
                {% endblock %}
            </div>

            <div class="content">
                {% block content %}
                {% endblock %}
            </div>
        </div>
    {% block javascripts %}{% endblock %}
</body>
</html>
